{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "One Tab",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon48.png"
  },
  "permissions": ["scripting", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
